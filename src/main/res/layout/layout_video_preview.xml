<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/ConstraintLayout.Video.Preview">
    <TextView style="@style/TextAppearance.Video.Preview.Label"/>
    <TextView style="@style/TextAppearance.Video.Preview.Seek"
              android:id="@+id/preview_video_seek"
              tools:text="00:32:50"/>
    <TextView style="@style/TextAppearance.Video.Preview.Split"
              android:id="@+id/preview_video_split"/>
    <TextView style="@style/TextAppearance.Video.Preview.Duration"
              android:id="@+id/preview_video_duration"
              tools:text="00:32:50"/>
    <SurfaceView style="@style/Surface.Video.Preview"
              android:id="@+id/preview_video_surface"/>
</android.support.constraint.ConstraintLayout>
