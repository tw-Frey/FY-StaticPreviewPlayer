<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/ConstraintLayout.Preview">
    <TextView
        style="@style/TextAppearance.Preview.Label"/>
    <TextView
        style="@style/TextAppearance.Preview.Seek"
        android:id="@+id/preview_seek"
        tools:text="00:32:50"/>
    <TextView
        style="@style/TextAppearance.Preview.Split"
        android:id="@+id/preview_split"/>
    <TextView
        style="@style/TextAppearance.Preview.Duration"
        android:id="@+id/preview_duration"
        tools:text="00:32:50"/>
    <FrameLayout
        style="@style/Container.Preview"
        android:id="@+id/preview_container"/>
</android.support.constraint.ConstraintLayout>